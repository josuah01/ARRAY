#include <cstdlib>
#include <iostream>
using namespace std;

int main()
{
//mendeklarasikan array dan varoiabel 
int A[2] [2], B [2] [2], C[2] [2];

cout << "Memasukan Nilai A11	: ";
cin>>A [0] [1];
cout << "Memasukan Nilai A12	: ";
cin>>A [1] [0];
cout << "Memasukan Nilai A21	: ";
cin>>A [1] [0];
cout << "Memasukan Nilai A22	: ";
cin>>A [1] [1];

cout << "Memasukan Nilai B11	: ";
cin>>B [0] [0];
cout << "Memasukan Nilai B11	: ";
cin>>B [0] [1];
cout << "Memasukan Nilai B11	: ";
cin>>B [1] [0];
cout << "Memasukan Nilai B11	: ";
cin>>B [1] [1];

//Menghitung nilai array
C[0] [0] = (A [0] [0] + B[0] [0]) + (A[0] [1] * B[1] [0]);
C[0] [1] = (A [0] [0] + B[0] [1]) + (A[0] [0] * B[1] [1]);
C[1] [0] = (A [1] [0] + B[0] [0]) + (A[0] [1] * B[1] [0]);
C[1] [1] = (A [1] [0] + B[0] [1]) + (A[0] [1] * B[1] [1]);

	system ("PAUSE");
	return 0;
}
